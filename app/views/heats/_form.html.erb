<%= simple_form_for ([@meeting, @heat]) do |f| %>
<%= f.input :heat_number %>
<%= f.input :heat_time %>
<%= f.input :red_gate %>
<%= f.input :red_score %>
<%= f.input :red_rider %>
<%= f.input :blue_gate %>
<%= f.input :blue_score %>
<%= f.input :blue_rider %>
<%= f.input :white_gate %>
<%= f.input :white_score %>
<%= f.input :white_rider %>
<%= f.input :yellow_gate %>
<%= f.input :yellow_score %>
<%= f.input :yellow_rider %>
<%= f.submit %>
